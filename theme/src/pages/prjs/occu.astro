---
import {Icon} from 'astro-icon'
import {
  AnnouncementBar,
	Button,
	CustomerQuoteSection,
	ThreeColumnTextSection,
	YouTubeEmbedSection,
  StickyTextImageSection,
  CtaCardSection
} from '@littlesticks/odyssey-theme-components';
import Layout from '../../layouts/Page.astro';
import TextAndImageHero from '../../components/sections/heros/TextAndImageHero.astro';

const seo = {
	title: 'Occupancy Sensors',
	description: 'Description of the work on Cccupancy Sensors',
};
---

<!-- <AnnouncementBar text="Go to the Monash website" href="https://www.monash.edu/smart-manufacturing-hub" slot="announcement-bar"/> -->
<Layout seo={seo} >
	<TextAndImageHero reversed>
		<div slot="text-container">
			<h1>Workspace utilization and management</h1>
			<p>The sub-project is designed and developed for the smart manufacturing workspace utilization and management. The precise data provides real-time insights for effective utilization, planning and management of manufacturing workspaces, which can boost the productivity.

			</p>
		</div>
		<div slot="btns-container">
			<Button href="http://13.55.132.220:3000/public/dashboard/d6083893-2a7e-487f-83b2-24f2ba5639d9" customIcon
				>To the dashboard <Fragment slot="icon">
          <Icon name="ic:baseline-chevron-right"/>
        </Fragment></Button
			>
		</div>
		<picture slot="image-container">
			<img
				src="/assets/images/smh_imgs/xysense.gif"
				alt="XY Sense"
				id="home-hero-image"
			/>
		</picture>
	</TextAndImageHero>  
</Layout>

